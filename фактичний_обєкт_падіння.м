дія _(ФактичнийОбєкт)
  дія ФактичнийОбєкт.падає()
    спробувати
      я.значення()
    зловити п
      якщо (я.навпаки)
        я.значення = "падає"
        я.провалити_припущення(я, "падає", "Очікується, що Дія чи функція не падає")
      кінець

      вернути
    кінець

    якщо (!я.навпаки)
      я.значення = "не падає"
      я.провалити_припущення(я, "падає", "Очікується, що Дія чи функція падає")
    кінець
  кінець
кінець

дати _